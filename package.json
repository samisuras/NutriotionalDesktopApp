{
  "name": "nutri",
  "description": "un fabuloso sistema nutricional",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "keywords": [],
  "author": "Samys",
  "license": "ISC",
  "build": {
    "appId": "nutri",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/image.ico"
    }
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "ejs-electron": "^2.0.3",
    "electron": "^4.0.0",
    "electron-reload": "^1.4.0",
    "express": "^4.17.1",
    "mysql": "^2.17.1",
    "open": "^7.0.2"
  },
  "devDependencies": {
    "electron-installer-windows": "^3.0.0",
    "electron-wix-msi": "^2.2.0"
  }
}
